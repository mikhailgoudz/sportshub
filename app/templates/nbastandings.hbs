 
 <div class="mt-4">
 <div class="text-center">
 <h2>NBA Standings</h2>
 <h5>Season</h5>
 <h6>2019-20</h6>
 </div>
 </div>

<div class = "mt-5">
<div class="container text-center">
<div class="row">
  <div class="col-6">
 <ul>
 <h3>Eastern Conference</h3>
  {{#each this.model as |team|}}
  
    {{#if team.IsEastern}}
      <li> <b>{{team.City}} {{team.Name}}</b>- <b> {{team.Wins}} - {{team.Losses}}</b></li>
    {{/if}}
  {{/each}}
</ul>

</div>
  <div class="col-6">

    <ul>
 <h3>Western Conference</h3>
  {{#each this.model as |team|}}
  
    {{#if team.IsWestern}}
      <li> <b>{{team.City}} {{team.Name}}</b>- <b> {{team.Wins}} - {{team.Losses}}</b></li>
 
    {{/if}}
  {{/each}}
</ul>


  </div>
</div>
</div>
</div>

{{outlet}}